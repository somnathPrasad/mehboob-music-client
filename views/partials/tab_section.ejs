<link rel="stylesheet" href="/css/tab.css">
<section class="et-hero-tabs mt-10">
            <div class="et-hero-tabs-container" style="background-color: #A9E4D7;">
                <a class="et-hero-tab" onclick="changeTabStatus(event,'youtube-section')" id="tab_youtube" href="#tab-es6">Youtube</a>
                <a class="et-hero-tab" onclick="changeTabStatus(event,'audio-section')" id="tab_audio" href="#tab-flexbox">Audio</a>
                <a class="et-hero-tab" onclick="changeTabStatus(event,'contact-section')" id="tab_contact" href="#tab-react">Contact</a>
              </div>
</section>
        <main class="et-main">
            <section class="et-slide" id="tab-es6" style="background-image: linear-gradient(#63B4B8, #fff);">
                <section id="youtube-section" class="youtube-section grid lg:grid-cols-2 grid-cols-1 items-center justify-center pt-10 pb-10">
                    <% videos.forEach(function(video){ %>
                        <%- include('video_tile', {id: video.id}); %>
                    <% }); %>
                </section>
            </section>
            <section class="et-slide" id="tab-flexbox">
                <section id="audio-section"></section>
            </section>
            <section class="et-slide" id="tab-react">
                <section id="contact-section" style="display: none;" class="contact-section flex flex-col justify-center items-center pt-10">
                    <div class="grid grid-cols-1">
                        <a class="flex items-center justify-center text-3xl px-5" href="mailto: mdarif42010@gmail.com"><img class="w-20" src="/images/gmail.png" alt="gmail"></a>
                        <a href="https://www.instagram.com/arif_syed9765" class="px-5 flex items-center justify-center"><img class="w-20 pt-5" src="/images/instagram.png" alt="instagram"></a>
                        <a href="#" class="px-5 flex items-center justify-center"><img class="w-20 pt-5" src="/images/linkedin.png" alt="linkedin"></a>
                        <a href="https://www.facebook.com/MehboobMusic/" class="px-5 flex items-center justify-center"><img class="w-20 pt-5" src="/images/facebook.png" alt="facebook"></a>
                    </div>
                    </section>  
            </section>
          </main>
</section>
<script
  src="https://code.jquery.com/jquery-3.6.0.slim.min.js"
  integrity="sha256-u7e5khyithlIdTpu22PHhENmPcRdFiHRjhAuHcs05RI="
  crossorigin="anonymous"></script>
